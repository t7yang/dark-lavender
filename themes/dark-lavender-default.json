{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Lavender",
  "type": "dark",
  "include": "dark-lavender-token.json",
  "colors": {
    /* Base colors */
    "focusBorder": "#6200EA", // purple-deep-a700
    "foreground": "#CFD8DC", // blue-gray-100
    "errorForeground": "#AD1457", // pink-800
    "icon.foreground": "#B388FF", // purple-deep-a100
    /* Text colors */
    /* Button control */
    "button.background": "#6200EA", // purple-deep-a700
    "checkbox.background": "#263238", // gray-900
    /* Dropdown control */
    "dropdown.background": "#263238", // gray-900
    "dropdown.listBackground": "#263238", // gray-900
    /* Input control */
    "input.background": "#263238", // gray-900
    "inputOption.activeBackground": "#6200EA", // purple-deep-a700
    "inputValidation.errorBackground": "#AD1457", // pink-800
    "inputValidation.errorBorder": "#AD1457", // pink-800
    "inputValidation.infoBackground": "#1565C0", // blue-800
    "inputValidation.infoBorder": "#1565C0", // blue-800
    "inputValidation.warningBackground": "#FF8F00", // amber-800
    "inputValidation.warningBorder": "#FF8F00", // amber-800
    /* Badge */
    "badge.background": "#6200EA", // purple-deep-a700
    /* Progress bar */
    "progressBar.background": "#6200EA", // purple-deep-a700
    /* Lists and trees */
    "list.dropBackground": "#17191F", // bg2
    "list.activeSelectionBackground": "#311B92", // purper-900
    "list.hoverBackground": "#17191F", // bg2
    "list.inactiveSelectionBackground": "#17191F", // bg2
    /* Activity Bar */
    "activityBar.background": "#07090F", // bg
    "activityBar.foreground": "#8C9EFF", // indigo-a100
    "activityBar.activeBorder": "#8C9EFF", // indigo-a100
    "activityBarBadge.background": "#6200EA", // purple-deep-a700
    /* Side Bar */
    "sideBar.background": "#07090F", // bg
    /* Editor Groups & Tabs */
    "editorGroupHeader.tabsBackground": "#07090F", // bg
    "tab.inactiveBackground": "#07090F", // bg
    "tab.activeBackground": "#07090F", // bg
    "tab.activeBorder": "#6200EA", // purple-deep-a700
    "tab.activeForeground": "#FAFAFA", // gray-50
    "tab.inactiveForeground": "#BDBDBD", // gray-500
    "tab.activeModifiedBorder": "#FF8F00", // amber-800
    /* Editor colors */
    "editor.foreground": "#CFD8DC", // blue-gray-100
    "editor.background": "#07090F", // bg
    "editorLineNumber.activeForeground": "#B388FF", // purple-deep-a100
    "editorIndentGuide.background": "#404040",
    "editorIndentGuide.activeBackground": "#707070",
    /* Diff editor colors */
    /* Editor widget colors */
    "editorWidget.background": "#07090F", // bg
    "editorWidget.border": "#6200EA", // purple-deep-a700
    /* Peek view colors */
    "peekView.border": "#6200EA", // purple-deep-a700
    "peekViewEditor.background": "#D1C4E905", // purple-deep-a100
    "peekViewResult.background": "#D1C4E905", // purple-deep-a100
    "peekViewEditor.matchHighlightBackground": "#D1C4E930", // purple-deep-a100
    "peekViewTitle.background": "#07090F", // bg
    /* Merge conflicts colors */
    /* Panel colors */
    "panelTitle.activeBorder": "#6200EA", // purple-deep-a700
    /* Status Bar colors */
    "statusBar.background": "#07090F", // bg
    "statusBar.debuggingBackground": "#BF360C", // oranged-900
    "statusBar.noFolderBackground": "#07090F", // bg
    "statusBarItem.prominentBackground": "#6200EA", // purple-deep-a700
    "statusBarItem.remoteBackground": "#6200EA", // purple-deep-a700
    /* Title Bar colors */
    "titleBar.activeBackground": "#07090F", // bg
    "titleBar.inactiveBackground": "#17191F", // bg2
    /* Menu Bar colors */
    "menu.background": "#17191F", // bg2
    /* Notification colors */
    "notificationToast.border": "#6200EA", // purple-deep-a700
    /* Extensions colors */
    "extensionButton.prominentBackground": "#6200EA", // purple-deep-a700
    "extensionButton.prominentHoverBackground": "#7C4DFF", // purple-deep-a200
    /* Integrated Terminal colors */
    /* Settings Editor colors */
    "settings.modifiedItemIndicator": "#FF8F00" // amber-800
  }
}

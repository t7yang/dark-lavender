{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Lavender",
  "type": "dark",
  "include": "dark-lavender-token.json",
  "colors": {
    /* Base colors */
    "focusBorder": "#6200EA", // purple-deep-a700
    "foreground": "#CFD8DC", // blue-gray-100
    "errorForeground": "#b71c1c", // red-900
    "icon.foreground": "#B388FF", // purple-deep-a100
    /* Text colors */
    /* Button control */
    "button.background": "#6200EA", // purple-deep-a700
    "checkbox.background": "#263238", // gray-900
    /* Dropdown control */
    "dropdown.background": "#263238", // gray-900
    "dropdown.listBackground": "#263238", // gray-900
    /* Input control */
    "input.background": "#263238", // gray-900
    "inputOption.activeBackground": "#6200EA", // purple-deep-a700
    "inputValidation.errorBackground": "#b71c1c", // red-900
    "inputValidation.errorBorder": "#b71c1c", // red-900
    "inputValidation.infoBackground": "#0D47A1", // blue-900
    "inputValidation.infoBorder": "#0D47A1", // blue-900
    "inputValidation.warningBackground": "#F57F17", // yellow-900
    "inputValidation.warningBorder": "#F57F17", // yellow-900
    /* Badge */
    "badge.background": "#6200EA", // purple-deep-a700
    /* Progress bar */
    "progressBar.background": "#6200EA", // purple-deep-a700
    /* Lists and trees */
    "list.dropBackground": "#111119", // bg2
    "list.activeSelectionBackground": "#311B92", // purper-900
    "list.hoverBackground": "#111119", // bg2
    "list.inactiveSelectionBackground": "#111119", // bg2
    /* Activity Bar */
    "activityBar.background": "#010109", // bg
    "activityBar.foreground": "#8C9EFF", // indigo-a100
    "activityBar.activeBorder": "#8C9EFF", // indigo-a100
    "activityBarBadge.background": "#6200EA", // purple-deep-a700
    /* Side Bar */
    "sideBar.background": "#010109", // bg
    /* Editor Groups & Tabs */
    "editorGroupHeader.tabsBackground": "#010109", // bg
    "tab.inactiveBackground": "#010109", // bg
    "tab.activeBackground": "#010109", // bg
    "tab.activeBorder": "#6200EA", // purple-deep-a700
    "tab.activeForeground": "#FAFAFA", // gray-50
    "tab.inactiveForeground": "#BDBDBD", // gray-500
    "tab.activeModifiedBorder": "#FF6F00", // amber-900
    /* Editor colors */
    "editor.foreground": "#CFD8DC", // blue-gray-100
    "editor.background": "#010109", // bg
    "editorLineNumber.foreground": "#455A64", // blue-gray-700
    "editorLineNumber.activeForeground": "#D500F9", // purple-a400
    "editorIndentGuide.background": "#37474F", // blue-gray-800
    "editorIndentGuide.activeBackground": "#78909C", // blue-gray-400
    "editorOverviewRuler.modifiedForeground": "#1A237E", // indigo-900
    "editorOverviewRuler.addedForeground": "#004D40", // teal-900
    "editorOverviewRuler.deletedForeground": "#880E4F", // pink-900
    "editorOverviewRuler.errorForeground": "#b71c1c", // red-900
    "editorOverviewRuler.warningForeground": "#F57F17", // yellow-900
    "editorOverviewRuler.infoForeground": "#0D47A1", // blue-900
    "editorGutter.modifiedBackground": "#1A237E", // indigo-900
    "editorGutter.addedBackground": "#004D40", // teal-900
    "editorGutter.deletedBackground": "#880E4F", // pink-900
    /* Diff editor colors */
    /* Editor widget colors */
    "editorWidget.background": "#010109", // bg
    "editorWidget.border": "#6200EA", // purple-deep-a700
    "editorMarkerNavigationError.background": "#b71c1c", // red-900
    "editorMarkerNavigationWarning.background": "#F57F17", // yellow-900
    "editorMarkerNavigationInfo.background": "#0D47A1", // blue-900
    /* Peek view colors */
    "peekView.border": "#6200EA", // purple-deep-a700
    "peekViewEditor.background": "#D1C4E905", // purple-deep-a100
    "peekViewResult.background": "#D1C4E905", // purple-deep-a100
    "peekViewEditor.matchHighlightBackground": "#D1C4E930", // purple-deep-a100
    "peekViewTitle.background": "#010109", // bg
    /* Merge conflicts colors */
    /* Panel colors */
    "panelTitle.activeBorder": "#6200EA", // purple-deep-a700
    /* Status Bar colors */
    "statusBar.background": "#010109", // bg
    "statusBar.debuggingBackground": "#BF360C", // oranged-900
    "statusBar.noFolderBackground": "#010109", // bg
    "statusBarItem.prominentBackground": "#6200EA", // purple-deep-a700
    "statusBarItem.remoteBackground": "#6200EA", // purple-deep-a700
    /* Title Bar colors */
    "titleBar.activeBackground": "#010109", // bg
    "titleBar.inactiveBackground": "#111119", // bg2
    /* Menu Bar colors */
    "menu.background": "#111119", // bg2
    /* Notification colors */
    "notificationToast.border": "#6200EA", // purple-deep-a700
    /* Extensions colors */
    "extensionButton.prominentBackground": "#6200EA", // purple-deep-a700
    "extensionButton.prominentHoverBackground": "#7C4DFF", // purple-deep-a200
    /* Integrated Terminal colors */
    /* Settings Editor colors */
    "settings.modifiedItemIndicator": "#FF6F00" // amber-900
  }
}

{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Lavender",
  "type": "dark",
  "include": "dark-lavender-token.json",
  "colors": {
    /* Base colors */
    "focusBorder": "#6200EA", // purple-deep-a700
    "foreground": "#CFD8DC", // blue-gray-100
    "errorForeground": "#C2185B", // pink-700
    "icon.foreground": "#B388FF", // purple-deep-a100
    /* Text colors */
    "textLink.activeForeground": "#FF9800", // orange-500
    "textLink.foreground": "#2196F3", // blue-500
    /* Button control */
    "button.background": "#6200EA", // purple-deep-a700
    "checkbox.background": "#263238", // gray-900
    "button.secondaryBackground": "#00B8D4", // cyan-a700
    /* Dropdown control */
    "dropdown.background": "#263238", // gray-900
    "dropdown.listBackground": "#263238", // gray-900
    /* Input control */
    "input.background": "#263238", // gray-900
    "inputOption.activeBackground": "#6200EA", // purple-deep-a700
    "inputValidation.errorBackground": "#880E4F", // pink-900
    "inputValidation.errorBorder": "#880E4F", // pink-900
    "inputValidation.infoBackground": "#0D47A1", // blue-900
    "inputValidation.infoBorder": "#0D47A1", // blue-900
    "inputValidation.warningBackground": "#E65100", // orange-900
    "inputValidation.warningBorder": "#E65100", // orange-900
    /* Badge */
    "badge.background": "#6200EA", // purple-deep-a700
    /* Progress bar */
    "progressBar.background": "#6200EA", // purple-deep-a700
    /* Lists and trees */
    "list.dropBackground": "#23252E", // bg2
    "list.activeSelectionBackground": "#311B92", // purper-900
    "list.hoverBackground": "#23252E", // bg2
    "list.inactiveSelectionBackground": "#23252E", // bg2
    /* Activity Bar */
    "activityBar.background": "#13151E", // bg
    "activityBar.inactiveForeground": "#EDE7F666", // deep-purple-50
    "activityBar.foreground": "#651FFF", // deep-purple-a400
    "activityBar.activeBorder": "#651FFF", // deep-purple-a400
    "activityBarBadge.background": "#6200EA", // deep-purple-a700
    /* Side Bar */
    "sideBar.background": "#13151E", // bg
    /* Editor Groups & Tabs */
    "editorGroupHeader.tabsBackground": "#13151E", // bg
    "tab.inactiveBackground": "#13151E", // bg
    "tab.activeBackground": "#13151E", // bg
    "tab.activeBorder": "#D500F9", // purple-a400
    "tab.activeForeground": "#FAFAFA", // gray-50
    "tab.inactiveForeground": "#BDBDBD", // gray-500
    "tab.activeModifiedBorder": "#FFA000", // amber-700
    /* Editor colors */
    "editor.foreground": "#CFD8DC", // blue-gray-100
    "editor.background": "#13151E", // bg
    "editorLineNumber.foreground": "#455A64", // blue-gray-700
    "editorLineNumber.activeForeground": "#D500F9", // purple-a400
    "editorIndentGuide.background": "#37474F", // blue-gray-800
    "editorIndentGuide.activeBackground": "#78909C", // blue-gray-400
    "editorOverviewRuler.modifiedForeground": "#0288D1", // light-blue-700
    "editorOverviewRuler.addedForeground": "#689F38", // light-green-700
    "editorOverviewRuler.deletedForeground": "#d32f2f", // red-700
    "editorOverviewRuler.errorForeground": "#C2185B", // pink-700
    "editorOverviewRuler.warningForeground": "#F57C00", // orange-700
    "editorOverviewRuler.infoForeground": "#1976D2", // blue-700
    "editorGutter.modifiedBackground": "#0288D1", // light-blue-700
    "editorGutter.addedBackground": "#689F38", // light-green-700
    "editorGutter.deletedBackground": "#d32f2f", // red-700
    /* Diff editor colors */
    "diffEditor.insertedTextBackground": "#8BC34A33", // light-green-500
    "diffEditor.removedTextBackground": "#f4433633", // red-500
    /* Editor widget colors */
    "editorWidget.background": "#13151E", // bg
    "editorWidget.border": "#6200EA", // purple-deep-a700
    "editorMarkerNavigationError.background": "#C2185B", // pink-700
    "editorMarkerNavigationWarning.background": "#F57C00", // orange-700
    "editorMarkerNavigationInfo.background": "#1976D2", // blue-700
    /* Peek view colors */
    "peekView.border": "#6200EA", // purple-deep-a700
    "peekViewEditor.background": "#6200EA05", // purple-deep-a700
    "peekViewResult.background": "#6200EA05", // purple-deep-a700
    "peekViewEditor.matchHighlightBackground": "#6200EA30", // purple-deep-a700
    "peekViewTitle.background": "#13151E", // bg
    /* Merge conflicts colors */
    "merge.currentHeaderBackground": "#00968899", // teal-500
    "merge.currentContentBackground": "#00968833", // teal-500
    "merge.incomingHeaderBackground": "#03A9F499", // light-blue-500
    "merge.incomingContentBackground": "#03A9F433", // light-blue-500
    /* Panel colors */
    "panelTitle.activeBorder": "#6200EA", // purple-deep-a700
    /* Status Bar colors */
    "statusBar.background": "#13151E", // bg
    "statusBar.debuggingBackground": "#BF360C", // oranged-900
    "statusBar.noFolderBackground": "#13151E", // bg
    "statusBarItem.prominentBackground": "#6200EA", // purple-deep-a700
    "statusBarItem.remoteBackground": "#6200EA", // purple-deep-a700
    /* Title Bar colors */
    "titleBar.activeBackground": "#13151E", // bg
    "titleBar.inactiveBackground": "#23252E", // bg2
    /* Menu Bar colors */
    "menu.background": "#23252E", // bg2
    /* Notification colors */
    "notificationToast.border": "#6200EA", // purple-deep-a700
    /* Extensions colors */
    "extensionButton.prominentBackground": "#6200EA", // purple-deep-a700
    "extensionButton.prominentHoverBackground": "#7C4DFF", // purple-deep-a200
    /* Integrated Terminal colors */
    "terminal.ansiBlack": "#263238", // blue-gray-900
    "terminal.ansiBlue": "#0D47A1", // blue-900
    "terminal.ansiBrightBlack": "#455A64", // blue-gray-700
    "terminal.ansiBrightBlue": "#2196F3", // blue-500
    "terminal.ansiBrightCyan": "#00BCD4", // cyan-500
    "terminal.ansiBrightGreen": "#4CAF50", // green-500
    "terminal.ansiBrightMagenta": "#E91E63", // pink-500
    "terminal.ansiBrightRed": "#f44336", // red-500
    "terminal.ansiBrightWhite": "#90A4AE", // blue-gray-300
    "terminal.ansiBrightYellow": "#FFEB3B", // yellow-500
    "terminal.ansiCyan": "#0097A7", // cyan-700
    "terminal.ansiGreen": "#388E3C", // green-700
    "terminal.ansiMagenta": "#C2185B", // pink-700
    "terminal.ansiRed": "#d32f2f", // red-700
    "terminal.ansiWhite": "#607D8B", // blue-gray-500
    "terminal.ansiYellow": "#FBC02D", // yellow-700
    /* Settings Editor colors */
    "settings.modifiedItemIndicator": "#FF6F00" // amber-900
  }
}

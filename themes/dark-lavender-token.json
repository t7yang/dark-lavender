{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Lavender",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Text",
      "scope": "comment.unused.elixir",
      "settings": {
        "foreground": "#CFD8DC" // bluegrey-100
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#607D8B" // bluegrey-500
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "constant.name.attribute.tag.pug",
        "keyword.control.ternary",
        "meta.brace.curly",
        "meta.brace.round",
        "meta.brace.square",
        "meta.decorator",
        "punctuation.accessor",
        "punctuation.brace",
        "punctuation.bracket",
        "punctuation.colon.graphql",
        "punctuation.comma.graphql",
        "punctuation.decorator",
        "punctuation.definition",
        "punctuation.definition.array",
        "punctuation.definition.entity",
        "punctuation.definition.parameters",
        "punctuation.definition.function",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.tag",
        "punctuation.definition.variable",
        "punctuation.delimiter.comma",
        "punctuation.delimiter.object.comma",
        "punctuation.parenthesis.begin",
        "punctuation.parenthesis.end",
        "punctuation.operation.graphql",
        "punctuation.other.comma",
        "punctuation.section.embedded",
        "punctuation.separator",
        "punctuation.separator.method",
        "punctuation.terminator.statement",
        "punctuation.separator.key-value",
        "punctuation.separator.dictionary",
        "punctuation.terminator",
        "punctuation.separator.comma",
        "punctuation.separator.array",
        "punctuation.section",
        "punctuation.section.property-list.begin.bracket.curly",
        "punctuation.section.property-list.end.bracket.curly",
        "punctuation.separator.statement",
        "punctuation.section.array.elixir",
        "punctuation.separator.object.elixir",
        "punctuation.section.embedded.elixir",
        "punctuation.section.function.elixir",
        "punctuation.section.scope.elixir",
        "punctuation.separator.parameter",
        "string.interpolated.pug",
        "support.function.interpolation.sass"
      ],
      "settings": {
        "foreground": "#8C9EFF" // indigo-a100
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#B388FF", // purpled-a100
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.operator.expression",
        "keyword.operator.new",
        "keyword.other.special-method.elixir",
        "meta.control.flow",
        "variable.language"
      ],
      "settings": {
        "foreground": "#B388FF", // purpled-a100
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage, Declaration Keywords",
      "scope": ["storage", "constant.language"],
      "settings": {
        "foreground": "#FF80AB", // pink-a100
        "fontStyle": "italic"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#64FFDA" // teal-a200
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "meta.method-call",
        "meta.require",
        "support.function",
        "support.function.any-method",
        "variable.function"
      ],
      "settings": {
        "foreground": "#64FFDA" // teal-a200
      }
    },
    {
      "name": "Methods",
      "scope": ["keyword.other.special-method"],
      "settings": {
        "foreground": "#64FFDA" // teal-a200
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.type.class",
        "entity.other.inherited-class",
        "meta.class.inheritance",
        "meta.class.instance",
        "storage.type.haskell",
        "support.class",
        "support.type.enum.graphql",
        "support.type.graphql",
        "variable.other.constant.elixir"
      ],
      "settings": {
        "foreground": "#FFFF8D" // yellow-a100
      }
    },
    {
      "name": "Type Primitive",
      "scope": ["support.type.primitive", "support.type.builtin"],
      "settings": {
        "foreground": "#FFFF8D" // yellow-a100
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#F0F4C3" // lime-100
      }
    },
    {
      "name": "Integers, Float",
      "scope": ["constant.numeric", "variable.other.anonymous.elixir"],
      "settings": {
        "foreground": "#F8BBD0" // pink-100
      }
    },
    {
      "name": "Strings, Inherited Class",
      "scope": [
        "punctuation.definition.string",
        "storage.type.string",
        "string",
        "string.quoted.double.shell",
        "support.constant.property-value",
        "support.function.variable.quoted.single.elixir"
      ],
      "settings": {
        "foreground": "#B2EBF2" // cyan-100
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#B9F6CA" // green-a100"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape", "constant.other.character-class"],
      "settings": {
        "foreground": "#FF5722" // oranged-500
      }
    },
    {
      "name": "Variables",
      "scope": [
        "source",
        "source.elixir.embedded.source",
        "string meta.embedded.line.ruby",
        "string source.groovy",
        "variable"
      ],
      "settings": {
        "foreground": "#FFAB91" // oranged-200
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language.symbol",
        "constant.other.symbol",
        "punctuation.definition.constant",
        "support.constant",
        "support.variable.magic.python",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#FFAB91" // oranged-200
      }
    },
    {
      "name": "Function Parameters",
      "scope": ["meta.function-call.arguments"],
      "settings": {
        "foreground": "#FFAB91" // oranged-200
      }
    },
    {
      "name": "Variable Parameter",
      "scope": [
        "parameter.variable.function.elixir",
        "variable.other.block.ruby",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FFAB91" // oranged-200
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "constant.other.key",
        "constant.other.object.key.js",
        "entity.name.tag.yaml",
        "meta.object-literal.key",
        "meta.object.member",
        "meta.meta.objectliteral",
        "meta.property-name",
        "string.unquoted.label.js",
        "support.variable.property",
        "support.type.map.key",
        "support.type.property-name",
        "variable.graphql",
        "variable.object.property",
        "variable.other.object.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#81D4FA" // bluel-200
      }
    },
    {
      "name": "XML/HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#B388FF" // purpled-a100
      }
    },
    {
      "name": "Attribute IDs",
      "scope": ["entity.other.attribute-name", "string.unquoted.alias.graphql"],
      "settings": {
        "foreground": "#FF80AB" // pink-a100
      }
    },
    {
      "name": "Selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "#9C27B0" // purple-500
      }
    },
    {
      "name": "Headings",
      "scope": [
        "entity.name.section",
        "markup.heading",
        "markup.heading.setext",
        "punctuation.definition.heading"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#64FFDA" // teal-a200
      }
    },
    {
      "name": "Bold",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F44336" // red-500
      }
    },
    {
      "name": "Italic",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "foreground": "#FF5722" // oranged-500
      }
    },
    {
      "name": "Code",
      "scope": ["markup.raw.inline"],
      "settings": {
        "foreground": "#64FFDA" // teal-a200
      }
    },
    {
      "name": "Quotes",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#B388FF" // purpled-a100
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#4CAF50" // green-500
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#F44336" // red-500
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#536DFE" // indigo-a200
      }
    },
    {
      "name": "Markup Code",
      "scope": ["markup.fenced_code.block", "markup.inline.raw", "markup.raw.block"],
      "settings": {
        "foreground": "#FF9800" // orange-500
      }
    },
    {
      "name": "Markup Link Image",
      "scope": ["markup.underline.link.image"],
      "settings": {
        "foreground": "#4CAF50" // green-500
      }
    },
    {
      "name": "Link Url",
      "scope": ["meta.link"],
      "settings": {
        "foreground": "#536DFE" // indigo-a200
      }
    },
    {
      "name": "Lists",
      "scope": ["beginning.punctuation.definition.list"],
      "settings": {
        "foreground": "#FF9800" // orange-500
      }
    },
    {
      "name": "Separator",
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#90A4AE" // bluegrey-300
      }
    },
    {
      "name": "Embedded",
      "scope": ["variable.interpolation"],
      "settings": {
        "foreground": "#536DFE" // indigo-a200
      }
    },
    {
      "name": "Illegal",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#C62828" // red-800
      }
    },
    {
      "name": "Css ID",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#F44336" // red-500
      }
    },
    {
      "name": "BASH: Command Substitution",
      "scope": ["string.interpolated.dollar.shell"],
      "settings": {
        "foreground": "#F44336" // red-500
      }
    },
    {
      "name": "BASH: Math Operation",
      "scope": ["string.other.math.shell"],
      "settings": {
        "foreground": "#3F51B5" // indigo 500
      }
    },
    {
      "name": "BASH: Substitution",
      "scope": [
        "punctuation.definition.string.begin.shell",
        "punctuation.definition.string.end.shell"
      ],
      "settings": {
        "foreground": "#9C27B0" // purple-500
      }
    },
    {
      "name": "CSV Rainbow 4",
      "scope": ["comment.rainbow4"],
      "settings": {
        "foreground": "#FF9800" // orange-500
      }
    },
    {
      "name": "CSV Rainbow 9",
      "scope": ["markup.bold.rainbow9"],
      "settings": {
        "foreground": "#673AB7", // purpled-500
        "fontStyle": ""
      }
    },
    {
      "name": "CSV Rainbow 10",
      "scope": ["invalid.rainbow10"],
      "settings": {
        "foreground": "#FF5722" // oranged-500
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#FFD180" // orange-a100
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFFF8D" // yellow-a100
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#CCFF90" // lgreen-a100
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A7FFEB" // teal-a100
      }
    },
    {
      "name": "JSON Key - Level 4",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#80D8FF" // lblue-a100
      }
    },
    {
      "name": "JSON Key - Level 5",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8C9EFF" // indigo-a100
      }
    },
    {
      "name": "JSON Key - Level 6",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#EA80FC" // purple-a100
      }
    },
    {
      "name": "JSON Key - Level 7",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FF80AB" // pink-a100
      }
    },
    {
      "name": "JSON Key - Level 8",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFD180" // orange-a100
      }
    }
  ]
}

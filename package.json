{
  "name": "dark-lavender",
  "displayName": "Dark Lavender",
  "description": "A smooth and nice dark theme for VSCode with material design colors.",
  "version": "6.2.0",
  "publisher": "t7yang",
  "engines": {
    "vscode": "^1.43.0"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#07090F",
    "theme": "dark"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark lavender",
    "dark",
    "material",
    "tailwindcss"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Dark Lavender",
        "uiTheme": "vs-dark",
        "path": "themes/default.json"
      },
      {
        "label": "Dark Lavender (Black)",
        "uiTheme": "vs-dark",
        "path": "themes/black.json"
      },
      {
        "label": "Dark Lavender (Soft)",
        "uiTheme": "vs-dark",
        "path": "themes/soft.json"
      },
      {
        "label": "Dark Lavender (Tailwind)",
        "uiTheme": "vs-dark",
        "path": "themes/tw.json"
      },
      {
        "label": "Dark Lavender (Tailwind Soft)",
        "uiTheme": "vs-dark",
        "path": "themes/tws.json"
      }
    ]
  },
  "license": "MIT",
  "homepage": "https://github.com/t7yang/dark-lavender",
  "repository": {
    "type": "git",
    "url": "https://github.com/t7yang/dark-lavender.git"
  },
  "bugs": {
    "url": "https://github.com/t7yang/dark-lavender/issues"
  },
  "scripts": {
    "update": "ncu -i --format group",
    "dev": "tsx watch src/themes.ts",
    "test": "tsc --noEmit",
    "build": "tsx src/themes.ts",
    "build:vs": "vsce package"
  },
  "devDependencies": {
    "@types/chroma-js": "^3.1.1",
    "@types/node": "^22.13.10",
    "chroma-js": "^3.1.2",
    "col-config": "^1.2.0",
    "material-colors-ts": "^1.0.4",
    "tailwindcss": "^4.0.14",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vscode-typed-theme-generator": "^4.0.1"
  }
}

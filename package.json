{
  "name": "dark-lavender",
  "displayName": "Dark Lavender",
  "description": "A smooth and nice dark theme for VSCode with material design colors.",
  "version": "6.1.0",
  "publisher": "t7yang",
  "engines": {
    "vscode": "^1.43.0"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#07090F",
    "theme": "dark"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark lavender",
    "dark",
    "material",
    "tailwindcss"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Dark Lavender",
        "uiTheme": "vs-dark",
        "path": "themes/default.json"
      },
      {
        "label": "Dark Lavender (Black)",
        "uiTheme": "vs-dark",
        "path": "themes/black.json"
      },
      {
        "label": "Dark Lavender (Soft)",
        "uiTheme": "vs-dark",
        "path": "themes/soft.json"
      },
      {
        "label": "Dark Lavender (Tailwind)",
        "uiTheme": "vs-dark",
        "path": "themes/tw.json"
      },
      {
        "label": "Dark Lavender (Tailwind Soft)",
        "uiTheme": "vs-dark",
        "path": "themes/tws.json"
      }
    ]
  },
  "license": "MIT",
  "homepage": "https://github.com/t7yang/dark-lavender",
  "repository": {
    "type": "git",
    "url": "https://github.com/t7yang/dark-lavender.git"
  },
  "bugs": {
    "url": "https://github.com/t7yang/dark-lavender/issues"
  },
  "scripts": {
    "update": "ncu -i --format group",
    "dev": "tsnd --respawn src/themes.ts",
    "build": "tsnd src/themes.ts",
    "build:vs": "vsce package"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.3",
    "@types/node": "^20.10.5",
    "chroma-js": "^2.4.2",
    "col-config": "^1.1.0",
    "material-colors-ts": "^1.0.4",
    "tailwindcss": "^3.3.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vscode-typed-theme-generator": "^2.0.6"
  }
}
